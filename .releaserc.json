{
  "branches": ["master"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "dist/atak-maps.zip",
            "name": "atak-maps-${nextRelease.version}.zip",
            "label": "All XML map layers"
          }
        ],
        "failComment": false
      }
    ]
  ]
}
